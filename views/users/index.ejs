<% include ../template/header.ejs %>
<% include ../template/nav.ejs %>
<%= info %>
<% user.forEach(function(user){%>
<div class="container">
  <div class="row">
    <div class="col-md-4 col-md-offset-4">
      <div class="well well-sm">
        <div class="media">
          <a class="thumbnail pull-left" href="#">
            <img class="media-object" src="<%= user.images%>">
          </a>
          <div class="media-body">
            <h4 class="media-heading"><%= user.fullName%></h4>
            <p><span class="label label-info"><%= user.email %></span> <span class="label label-warning">Followers</span></p>
            <p>
              <a href="#" class="btn btn-xs btn-default"><span class="glyphicon glyphicon-comment"></span> Message</a>
              <a href="#" class="btn btn-xs btn-default"><span class="glyphicon glyphicon-heart"></span> Following</a>
              <a href="#" class="btn btn-xs btn-default"><span class="glyphicon glyphicon-ban-circle"></span> Unfollow</a>
            </p>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<%})%>
<% include ../template/footer.ejs %>